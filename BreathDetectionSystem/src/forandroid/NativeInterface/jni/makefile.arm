#CFLAGS+=-lFCNetAC -L$(MY_PATH) -lgnustl_shared
#CFLAGS+=-L$(MY_PATH) -lgnustl_shared
all:$(OBJS) $(TARGET)

%.o:%.cpp include/%.h 
	$(CC) $< $(CCFLAGS) -o $@

