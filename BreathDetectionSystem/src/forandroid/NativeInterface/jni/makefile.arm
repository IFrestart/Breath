LIBOBJS=../../lib/libFCNetAC.a ../../lib/libNetCtrl.a $(ADDONSOBJS) $(ACE_ROOT)/ace/libACE.a
MY_PATH=$(NDK_HOME)/sources/cxx-stl/gnu-libstdc++/4.4.3/libs/armeabi

OBJS=$(patsubst %.cpp,%.o,$(wildcard *.cpp))
