include makefile.def

DIRS =	\
	socks5	\
	jrtplib-2.7	\
	tinyxmllib	\
	CreateKey	\
	\
	CommBase	\
	ServBase	\
	SendConfig	\
	CenterCtrlServ	\
	SubCtrlServ	\
	AuthDBCommServ	

all:
	for i in $(DIRS) ; do \
		if [ -d $$i ]; then	\
			(cd $$i; make) ;	\
		else	\
			echo "----------------------------------------" ;	\
			echo "$$i is not a directory..." ;	\
			echo "----------------------------------------" ;	\
		fi	\
	done

clean:
	for i in $(DIRS) ; do \
		if [ -d $$i ]; then	\
			(cd $$i; make clean) ;	\
		else	\
			echo "----------------------------------------" ;	\
			echo "$$i is not a directory..." ;	\
			echo "----------------------------------------" ;	\
		fi	\
	done

